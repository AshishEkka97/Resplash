<?xml version="1.0" encoding="utf-8"?>
<resources>

    <attr name="alphaEmphasisHigh" format="float"/>
    <attr name="alphaEmphasisMedium" format="float"/>
    <attr name="alphaEmphasisDisabled" format="float"/>

    <declare-styleable name="ExpandableCardView">
        <attr name="header_view" format="reference"/>
        <attr name="content_view" format="reference"/>
        <attr name="expanded" format="boolean"/>
        <attr name="animation_duration" format="integer"/>
    </declare-styleable>

    <declare-styleable name="TopicStatusView">
        <attr name="status" format="enum">
            <enum name="open" value="0" />
            <enum name="closed" value="1" />
        </attr>
    </declare-styleable>
</resources>
